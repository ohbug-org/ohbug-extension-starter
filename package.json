{
  "name": "ohbug-extension-starter",
  "version": "0.0.0",
  "description": "A simple starter to get up quickly with Ohbug Extension",
  "license": "MIT",
  "author": "chenyueban <jasonchan0527@gmail.com>",
  "homepage": "https://github.com/ohbug-org/ohbug-extension-starter",
  "bugs": {
    "url": "https://github.com/ohbug-org/ohbug-extension-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohbug-org/ohbug-extension-starter"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "release": "release-it",
    "preinstall": "npx only-allow pnpm",
    "update:deps": "pnpm update -i -r --latest",
    "lint": "tsc && eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ohbug/types": "^2.1.0"
  },
  "devDependencies": {
    "@chenyueban/eslint-config": "^1.0.12",
    "@chenyueban/tsconfig": "^2.1.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@release-it/conventional-changelog": "^5.0.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "release-it": "^15.1.4",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.0",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
